generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model admin {
  id       Int    @id @default(autoincrement())
  name     String
  password String
}

model user {
  id          Int     @id @default(autoincrement())
  name        String
  phoneNumber String?
  oilAmount   Int
  bikeNumber  String?
}
